                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/sale_inproduct3_linear"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="进购量(KG)："
                    android:textSize="16dp"
                    android:textColor="@android:color/black"/>
                <com.xw.repo.BubbleSeekBar
                    android:id="@+id/sale_inproduct3_seekbar"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    app:bsb_bubble_color="@color/colorPrimary"
                    app:bsb_bubble_text_color="@color/colorPrimaryHalf"
                    app:bsb_max="50"
                    app:bsb_min="0"
                    app:bsb_progress="0"
                    app:bsb_second_track_color="@android:color/holo_green_dark"
                    app:bsb_section_count="5"
                    app:bsb_section_text_position="bottom_sides"
                    app:bsb_show_progress_in_float="true"
                    app:bsb_show_section_mark="true"
                    app:bsb_show_section_text="true"
                    app:bsb_show_thumb_text="true"
                    app:bsb_track_color="@android:color/holo_green_light"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/sale_inproduct4_linear"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/sale_inproduct3_volumn_linear">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="伊利——纯牛奶"
                    android:textSize="16dp"
                    android:textColor="@android:color/black"/>
                <com.suke.widget.SwitchButton
                    android:id="@+id/sale_inproduct4_switchbtn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/sale_inproduct4_volumn_linear"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/sale_inproduct4_linear"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="进购量(KG)："
