<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="match_parent" android:layout_width="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:id="@+id/sale_orgcode_lienar"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <android.support.design.widget.TextInputLayout
                    android:id="@+id/sale_orgcode_textinput"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp">
                    <android.support.design.widget.TextInputEditText
                        android:id="@+id/sale_orgcode_edittext"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        tools:text="零售企业选择"
                        tools:textColor="@android:color/black"
                        android:hint="零售企业选择" />

                </android.support.design.widget.TextInputLayout>

                <ImageButton
                    android:id="@+id/sale_orgacode_select_imgbtn"
                    android:src="@drawable/plus"
                    android:layout_marginEnd="5dp"
                    android:background="@android:color/transparent"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>


            <LinearLayout
                android:id="@+id/sale_batchid_linear"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/sale_orgcode_lienar">
                <android.support.design.widget.TextInputLayout
                    android:id="@+id/sale_batchid_textinput"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp">
                    <android.support.design.widget.TextInputEditText
                        android:id="@+id/sale_batchid_edittext"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        tools:text="产品批次码选择"
                        tools:textColor="@android:color/black"
                        android:hint="产品批次码选择" />

                </android.support.design.widget.TextInputLayout>

                <ImageButton
                    android:id="@+id/sale_batchid_imgbtn"
                    android:src="@drawable/plus"
                    android:layout_marginEnd="5dp"
                    android:background="@android:color/transparent"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/sale_intime_linear"
                android:layout_below="@+id/sale_batchid_linear"
                android:layout_marginTop="20dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <android.support.design.widget.TextInputLayout
                    android:id="@+id/sale_intime_textinput"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content">
                    <android.support.design.widget.TextInputEditText
                        android:id="@+id/sale_intime_edittext"
                        android:hint="选择进货时间"
                        tools:text="选择进货时间"
                        tools:textColor="@android:color/black"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"/>
                </android.support.design.widget.TextInputLayout>
                <ImageButton
                    android:id="@+id/sale_intime_imgbtn"
                    android:src="@drawable/calendar"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <TextView
                android:id="@+id/sale_inproduct_text"
                android:layout_below="@+id/sale_intime_linear"
                android:layout_marginTop="20dp"
                android:text="进购产品选择："
                android:textColor="@android:color/black"
                android:textSize="20dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <LinearLayout
                android:id="@+id/sale_inproduct1_linear"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="20dp"
                android:layout_below="@+id/sale_inproduct_text">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="蒙牛——纯牛奶"
                    android:textSize="16dp"
                    android:textColor="@android:color/black"/>
                <com.suke.widget.SwitchButton
                    android:id="@+id/sale_inproduct1_switchbtn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/sale_inproduct1_volumn_linear"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/sale_inproduct1_linear"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="进购量(KG)："
                    android:textSize="16dp"
                    android:textColor="@android:color/black"/>
                <com.xw.repo.BubbleSeekBar
                    android:id="@+id/sale_inproduct1_seekbar"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    app:bsb_bubble_color="@color/colorPrimary"
                    app:bsb_bubble_text_color="@color/colorPrimaryHalf"
                    app:bsb_max="50"
                    app:bsb_min="0"
                    app:bsb_progress="0"
                    app:bsb_second_track_color="@android:color/holo_green_dark"
                    app:bsb_section_count="5"
                    app:bsb_section_text_position="bottom_sides"
                    app:bsb_show_progress_in_float="true"
                    app:bsb_show_section_mark="true"
                    app:bsb_show_section_text="true"
                    app:bsb_show_thumb_text="true"
                    app:bsb_track_color="@android:color/holo_green_light"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/sale_inproduct2_linear"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/sale_inproduct1_volumn_linear">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="蒙牛——纯甄"
                    android:textSize="16dp"
                    android:textColor="@android:color/black"/>
                <com.suke.widget.SwitchButton
                    android:id="@+id/sale_inproduct2_switchbtn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/sale_inproduct2_volumn_linear"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/sale_inproduct2_linear"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="进购量(KG)："
                    android:textSize="16dp"
                    android:textColor="@android:color/black"/>
                <com.xw.repo.BubbleSeekBar
                    android:id="@+id/sale_inproduct2_seekbar"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    app:bsb_bubble_color="@color/colorPrimary"
                    app:bsb_bubble_text_color="@color/colorPrimaryHalf"
                    app:bsb_max="50"
                    app:bsb_min="0"
                    app:bsb_progress="0"
                    app:bsb_second_track_color="@android:color/holo_green_dark"
                    app:bsb_section_count="5"
                    app:bsb_section_text_position="bottom_sides"
                    app:bsb_show_progress_in_float="true"
                    app:bsb_show_section_mark="true"
                    app:bsb_show_section_text="true"
                    app:bsb_show_thumb_text="true"
                    app:bsb_track_color="@android:color/holo_green_light"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/sale_inproduct3_linear"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/sale_inproduct2_volumn_linear">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="蒙牛——真果粒"
                    android:textSize="16dp"
                    android:textColor="@android:color/black"/>
                <com.suke.widget.SwitchButton
                    android:id="@+id/sale_inproduct3_switchbtn"
