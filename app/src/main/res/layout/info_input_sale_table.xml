                    android:id="@+id/sale_contact_person_phone_inputlayout"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content">
                    <android.support.design.widget.TextInputEditText
                        android:id="@+id/sale_contact_person_phone_edittext"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="经销商联系人电话"
                        tools:text="经销商联系人电话"
                        tools:textColor="@android:color/black"/>
                </android.support.design.widget.TextInputLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/sale_phone_certificate_code_linear"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_below="@+id/sale_contace_person_phone_linear">
                <android.support.design.widget.TextInputLayout
                    android:id="@+id/sale_certificate_code_inputlayout"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content">
                    <android.support.design.widget.TextInputEditText
                        android:id="@+id/sale_certificate_code_edittext"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="输入验证码"
                        tools:text="输入验证码"
                        tools:textColor="@android:color/black"/>
                </android.support.design.widget.TextInputLayout>

                <Button
                    android:id="@+id/sale_phone_getcertificatecode_btn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="获取验证码"
                    android:textColor="@android:color/black"
                    android:textSize="14dp"/>

            </LinearLayout>

            <com.andexert.library.RippleView
                android:id="@+id/sale_rippleview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/sale_phone_certificate_code_linear"
                android:layout_marginTop="20dp"
                app:rv_centered="true">
                <Button
                    android:id="@+id/sale_upload_button"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@android:color/black"
                    android:text="提交表单"
                    android:textSize="17dp"
                    android:background="@android:color/holo_blue_dark"/>
            </com.andexert.library.RippleView>

        </RelativeLayout>
    </ScrollView>


</RelativeLayout>
