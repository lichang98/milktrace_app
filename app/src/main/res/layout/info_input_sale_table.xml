            <android.support.design.widget.TextInputLayout
                android:id="@+id/sale_contact_personname_inputlayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/sale_inproduct6_volumn_linear">
                <android.support.design.widget.TextInputEditText
                    android:id="@+id/sale_contact_personname_edittext"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="经销商联系人姓名"
                    tools:text="经销商联系人姓名"
                    tools:textColor="@android:color/black"/>

            </android.support.design.widget.TextInputLayout>

            <LinearLayout
                android:id="@+id/sale_contace_person_phone_linear"
                android:orientation="horizontal"
                android:layout_below="@+id/sale_contact_personname_inputlayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
